function awslog(url){var n="t_"+(new Date()).getTime()+Math.random()*9999;var c=window[n]=new Image(1,1);c.onload=(c.onerror=function(){window[n]=null});c.src=url;url=null;n=null;c=null};awslog('http://cm.g.doubleclick.net/pixel?google_nid=WIS&google_cm');awslog('http://sax.sina.com.cn/cm?sina_nid=12');;